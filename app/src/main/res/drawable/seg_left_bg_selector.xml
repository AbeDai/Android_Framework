<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape >
            <stroke android:color="#ffffff" android:width="3px"/>
            <solid android:color="#21c39c"/>
            <corners android:topLeftRadius="10dp" android:bottomLeftRadius="10dp" android:topRightRadius="0dp" android:bottomRightRadius="0dp"/>
        </shape>
    </item>
    <item>
        <shape >
            <stroke android:color="#ffffff" android:width="3px"/>
            <solid android:color="#00000000"/>
            <corners android:topLeftRadius="10dp" android:bottomLeftRadius="10dp" android:topRightRadius="0dp" android:bottomRightRadius="0dp"/>
        </shape>
    </item>
</selector>

